<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>

	<div id="result-area"></div>
	<button id="req-btn">데이터요청</button>
	
	<script src="js/jquery-3.7.0.js"></script>
	<script>
		$("#req-btn").on("click", function(){
			
			$.ajax({
				url:"JavaToJson",
				success:function(res){
					let resultHTML ="";
					
					for(let i = 0; i < res.length; i++){
						let coffee = res[i];
						let menuNm = coffee.menuNm;
						let price = coffee.price;
						let size = coffee.size;
						resultHTML += "<p>" + menuNm + " | " + price + " | " + size + "</p>";
					}
					$("#result-area").html(resultHTML);
				},
				error:function(){
					alert("요청실패...");
				}
			}); // ajax end
		}); // event end
	
	
	</script>
	
</body>
</html>